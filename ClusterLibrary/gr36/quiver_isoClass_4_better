digraph G {
    "0" [color = cyan,penwidth="5", label = "0"]
    "2" [color = cyan,penwidth="5", label = "2"]
    "3" [color = cyan,penwidth="5", label = "3"]
    "1" [color = cyan,penwidth="5", label = "1"]

    "3" -> "0" [len = 2.0,  color = "black"]
    "2" -> "3" [len = 2.0,  color = "black"]
    "1" -> "2" [len = 2.0,  color = "black"]
    "0" -> "1" [len = 2.0,  color = "black"]



}